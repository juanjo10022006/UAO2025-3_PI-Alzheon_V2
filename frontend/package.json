{
  "name": "alzheon-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.js'",
    "test:paciente": "cypress run --spec 'cypress/e2e/paciente/**/*.cy.js'",
    "test:cuidador": "cypress run --spec 'cypress/e2e/cuidador/**/*.cy.js'",
    "test:medico": "cypress run --spec 'cypress/e2e/medico/**/*.cy.js'",
    "test:integration": "cypress run --spec 'cypress/e2e/integration/**/*.cy.js'",
    "test:critical": "cypress run --spec 'cypress/e2e/cuidador/upload-photo.cy.js,cypress/e2e/integration/main-flow.cy.js'"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@reduxjs/toolkit": "^2.7.0",
    "@tailwindcss/vite": "^4.1.4",
    "axios": "^1.13.2",
    "cva": "npm:class-variance-authority@^0.7.1",
    "dotenv": "^16.5.0",
    "firebase": "^11.8.1",
    "framer-motion": "^12.11.4",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-force-graph": "^1.47.6",
    "react-force-graph-2d": "^1.27.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "recharts": "^2.15.4",
    "sonner": "^2.0.3",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/cypress": "^10.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cypress": "^1.1.6",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.4.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "patch-package": "^8.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
